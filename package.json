{
  "name": "finecss",
  "version": "0.13.0",
  "description": "Small, opinionated CSS framework to make sites look alright with minimal effort",
  "author": "Andreas Philippi",
  "license": "MIT",
  "type": "module",
  "main": "scripts/fine.js",
  "style": "dist/index.min.css",
  "repository": "https://github.com/andreasphil/fine",
  "files": [
    "dist/index.min.css",
    "scripts/fine.js"
  ],
  "scripts": {
    "dev": "npm run build:styles -- --watch",
    "docs": "wmr",
    "build:styles": "postcss src/index.css -o dist/index.min.css",
    "build:docs": "wmr build",
    "preversion": "npm run build:styles; git add --force dist/index.min.css"
  },
  "devDependencies": {
    "@mdx-js/rollup": "^2.3.0",
    "@types/mdx": "^2.0.4",
    "postcss": "^8.4.22",
    "postcss-cli": "^10.1.0",
    "postcss-csso": "^6.0.1",
    "postcss-import": "^15.1.0",
    "postcss-inline-svg": "^6.0.0",
    "postcss-nesting": "^11.2.2",
    "preact": "^10.11.3",
    "preact-iso": "^2.3.1",
    "wmr": "^3.8.0"
  }
}
